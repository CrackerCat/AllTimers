<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="MaxHeightScrollView" tools:ignore="ResourceName">
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DynamicDisplayView" tools:ignore="ResourceName">
        <attr name="text" format="string" />
        <attr name="textSize" format="integer" />
    </declare-styleable>
    <declare-styleable name="DynamicBackgroundFrameLayout" tools:ignore="ResourceName">
        <attr name="background_src" format="reference" />
    </declare-styleable>
    <declare-styleable name="Selector" tools:ignore="ResourceName">
        <attr name="hint" format="string" />
        <attr name="hintSize" format="integer" />
        <attr name="icSize" format="integer" />
        <attr name="iconDrawable" format="reference" />
        <attr name="selectDrawable" format="reference" />
        <attr name="content_padding" format="integer" />
        <attr name="unselectDrawable" format="reference" />
        <attr name="isSingleSelect" format="boolean" />
        <attr name="isSelect" format="boolean" />
        <attr name="hintColor" format="color" />
        <attr name="check_icon_position" format="integer">
<!--            <enum name="center_right" value="center_right" />-->
<!--            <enum name="center_left" value="1" />-->
<!--            <enum name="right" value="2" />-->
<!--            <enum name="left" value="left" />-->
        </attr>
    </declare-styleable>
</resources>