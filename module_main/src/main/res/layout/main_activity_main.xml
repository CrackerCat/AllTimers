<?xml version="1.0" encoding="utf-8"?>
<com.sandyz.alltimers.main.view.costom.DynamicBackgroundFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFDFD7"
    app:background_src="@drawable/main_ic_bg_rabbit">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.ActivityMain">

        <androidx.viewpager2.widget.ViewPager2
            android:id="@+id/vp_main"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@+id/main_constraintlayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/main_constraintlayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:clipChildren="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <com.sandyz.alltimers.main.view.costom.BottomNavButton
                android:id="@+id/bottom_navi_schedule"
                android:layout_width="38dp"
                android:layout_height="57dp"
                android:layout_marginBottom="8dp"
                android:background="?selectableItemBackgroundBorderless"
                app:checked_src="@drawable/main_ic_mine"
                app:label="日程"
                app:label_color="#995B3A"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/bottom_navi_concentrate"
                app:layout_constraintStart_toStartOf="parent"
                app:no_checked_src="@drawable/main_ic_schedule" />

            <com.sandyz.alltimers.main.view.costom.BottomNavButton
                android:id="@+id/bottom_navi_concentrate"
                android:layout_width="38dp"
                android:layout_height="0dp"
                android:background="?selectableItemBackgroundBorderless"
                app:checked_src="@drawable/main_ic_mine"
                app:label="专注"
                app:label_color="#995B3A"
                app:layout_constraintBottom_toBottomOf="@+id/bottom_navi_schedule"
                app:layout_constraintEnd_toStartOf="@+id/main_bottom_navi_concentrate_mine2"
                app:layout_constraintStart_toEndOf="@+id/bottom_navi_schedule"
                app:layout_constraintTop_toTopOf="@+id/bottom_navi_schedule"
                app:no_checked_src="@drawable/main_ic_concentrate" />

            <com.sandyz.alltimers.main.view.costom.BottomNavButton
                android:id="@+id/bottom_navi_concentrate_mine"
                android:layout_width="38dp"
                android:layout_height="0dp"
                android:background="?selectableItemBackgroundBorderless"
                app:checked_src="@drawable/main_ic_mine"
                app:label="我的"
                app:label_color="#995B3A"
                app:layout_constraintBottom_toBottomOf="@+id/main_bottom_navi_concentrate_mine"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/main_bottom_navi_concentrate_mine"
                app:layout_constraintTop_toTopOf="@+id/main_bottom_navi_concentrate_mine"
                app:no_checked_src="@drawable/main_ic_mine" />

            <com.sandyz.alltimers.main.view.costom.BottomNavButton
                android:id="@+id/main_bottom_navi_concentrate_mine"
                android:layout_width="38dp"
                android:layout_height="0dp"
                android:background="?selectableItemBackgroundBorderless"
                app:checked_src="@drawable/main_ic_mine"
                app:label="日历"
                app:label_color="#995B3A"
                app:layout_constraintBottom_toBottomOf="@+id/bottom_navi_concentrate"
                app:layout_constraintEnd_toStartOf="@+id/bottom_navi_concentrate_mine"
                app:layout_constraintStart_toEndOf="@+id/main_bottom_navi_concentrate_mine2"
                app:layout_constraintTop_toTopOf="@+id/bottom_navi_concentrate"
                app:no_checked_src="@drawable/main_ic_calendar" />

            <com.sandyz.alltimers.main.view.costom.BottomNavButton
                android:id="@+id/main_bottom_navi_concentrate_mine2"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="24dp"
                android:background="?selectableItemBackgroundBorderless"
                app:checked_src="@drawable/main_ic_add"
                app:label_color="#995B3A"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:no_checked_src="@drawable/main_ic_add" />
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</com.sandyz.alltimers.main.view.costom.DynamicBackgroundFrameLayout>
